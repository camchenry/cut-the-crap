{
  "manifest_version": 2,
  "name": "Cut the Crap",
  "version": "1.0",
  "description": "Removes distracting content from webpages",
  "icons": {
    "48": "icons/scissor-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["twitter.js"]
    },
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["youtube.js"]
    }
  ]
}
